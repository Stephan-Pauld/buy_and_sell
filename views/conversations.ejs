  <%- include('header') %>
  <div class='conversations'>
    <div class="item-id">
      <% for(const group of groups) { %>
      </br>
      <p class='item-name'><%= group[0].item%></p>
      <div id=<%=group[0].id%>class="messages">
        <% for(const message of group) { %>
        <div class='messageLine'>
          <span><%= moment(message.date).format('Do MMM, YYYY')%> </span><span><%=message.from%> </span><span><%=message.message%>></span></div>
        <%}%>
      <%}%>
      </div>
    </div>
  </div>
