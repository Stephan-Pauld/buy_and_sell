<div class="card-deck">
  <% for(let i = 0; i <= 2; i++) { %>
  <div class="card">
    <img src="https://images.pexels.com/photos/2086676/pexels-photo-2086676.jpeg?auto=compress&amp;cs=tinysrgb&amp;h=350" class="card-img-top">
    <div class="card-body border-info mb-3">
      <h5 class="card-title">Iphone6</h5>
      <p class="card-text"> Price : $20</p>
      <p class="card-text"> Location?</p>
    </div>
    <div class="card-footer">
      <small class="text-muted">posted 3 days ago</small>
    </div>
  </div>
  <% }%>

</div>
